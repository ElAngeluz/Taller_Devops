{
  "Serilog": {
    "MinimumLevel": {
      "Default": "Information",
      "Override": {
        "Microsoft": "Warning",
        "System": "Warning"
      }
    },
    "AllowedHosts": "*",
    "Enrich": [ "FromLogContext", "WithMachineName" ],
    "WriteTo": [
      {
        "Name": "Console"
      },
      {
        "Name": "File",
        "Args": {
          "path": "./logs/taller.devops..log",
          "rollingInterval": "Day",
          "shared": "true"
        }
      }
    ],
    "Properties": {
      "Application": "Taller.devops.api"
    }
  },
  "Jaeger": {
    "Telemetry": {
      "Host": "localhost",
      "Port": 6831
    }
  },
  "OpenApi": {
    "info": {
      "description": "Taler DevOps",
      "version": "v1",
      "title": "Taller DEVOPS",
      "termsOfService": "http://localhost/terms.html",
      "contact": {
        "name": "Pedro Abel",
        "url": "https://pagina.com",
        "email": "parivera@angeluzcorp.com"
      },
      "license": {
        "name": "Pedro Abel",
        "url": "http://localhost/license.html"
      }
    }
  }
}
